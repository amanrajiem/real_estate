<p id="notice"><%= notice %></p>

<div class="container">


<h1>Commecial Units</h1>

<table class="table table-bordered text-center">
  <thead>
    <tr>
      <th>Owner</th>
      <th>Address</th>
      <th>Shops</th>
      <th>Sqmt</th>
      <th>Parking</th>
      <th>Price</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @commecial_units.each do |commecial_unit| %>
      <tr>
        <td><%= commecial_unit.owner %></td>
        <td><%= commecial_unit.address %></td>
        <td><%= commecial_unit.shops %></td>
        <td><%= commecial_unit.sqmt %></td>
        <td><%= commecial_unit.parking %></td>
        <td><%= commecial_unit.price %></td>
        <td><%= link_to 'Show', commecial_unit %></td>
        <td><%= link_to 'Edit', edit_commecial_unit_path(commecial_unit) %></td>
        <td><%= link_to 'Destroy', commecial_unit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commecial Unit', new_commecial_unit_path, class: 'btn btn-info' %>
